<h1>Listing posts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Permalink</th>
    <th>Status</th>
    <th>Active</th>
    <th>Published at</th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%=link_to h(post.title), post %></td>
    <td><%=h post.description %></td>
    <td><%#=h post.permalink %></td>
    <td><%#=h post.status %></td>
    <td><%#=h post.active %></td>
    <td><%=h post.published_at %></td>
    <td><%= link_to 'Show', post %></td>
  </tr>
<% end %>
</table>